<p id="notice"><%= notice %></p>

<h1>Education2s</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @education2s.each do |education2| %>
      <tr>
        <td><%= education2.body %></td>
        <td><%= education2.title %></td>
        <td><%= link_to 'Show', education2 %></td>
        <td><%= link_to 'Edit', edit_education2_path(education2) %></td>
        <td><%= link_to 'Destroy', education2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education2', new_education2_path %>
